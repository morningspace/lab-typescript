{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "import-name": false,
    "strict-boolean-expressions": false,
    "variable-name": [true, "allow-pascal-case"],
    "quotemark": [true, "single"],
    "no-empty": [true, "allow-empty-functions"],
    "interface-name": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
