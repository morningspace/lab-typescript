{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Task": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "find": true,
          "prototype.patchAttributes": true,
          "deleteById": true,
          "getList": true,
          "search": true
        }
      }
    }
  },
  "SharableTask": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "find": true,
          "prototype.patchAttributes": true,
          "deleteById": true,
          "getList": true,
          "search": true
        }
      }
    }
  },
  "Note": {
    "dataSource": "db",
    "public": false
  }
}
